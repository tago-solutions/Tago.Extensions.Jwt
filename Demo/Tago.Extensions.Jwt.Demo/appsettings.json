{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FileLoggingOptions": {
    "BatchSize": 10,
    "IsEnabled": true,
    "FileSizeLimit": 10485760,
    "RetainedFileCountLimit": 2,
    "FileName": "test-app.log",
    "LogDirectory": "Logs",
    "FilePerDay": false,
    "Encoding": "Utf8"
  },
  "Jwt": {
    "Settings": {
      "DefaultKid": "_default_",
      "ValidatorKey": "%kid%",
      "SecurityKeysCacheExpiration": "00:05:00",
      "Keys": {
        "test": {
          "SignerSettings": {
            "Issuer": "me",
            "Audience": "me",
            "KeySettings": {
              "SymmetricKey": {
                "Key": "veryVerySecretKey",
                "SecurityAlgorithm": "HS256"
              }
            }
          },
          "ValidationSettings": {
            "ValidIssuer": "me",
            "ValidAudience": "me",
            "ValidateIssuer": true,
            "ValidateAudience": true,
            "ValidateLifetime": true,
            "KeySettings": {
              "SymmetricKey": {
                "Key": "veryVerySecretKey",
                "SecurityAlgorithm": "HS256"
              }
            }
          }
        }
      }
    },
    "Validators": {
      "test_me": [
        {
          "Fields": [
            {
              "Type": "Issuer",
              "Mandatory": false,
              "MatchType": "Contains",
              "Values": [
                "me*",
                "meee"
              ],
              "IgnoreCase": true
            },
            {
              "Type": "Audience",
              "Mandatory": false,
              "MatchType": "NotContains",
              "Values": [
                "me*",
                "meee"
              ],
              "IgnoreCase": false
            }
          ]
        }
      ]
    }
  }
}
